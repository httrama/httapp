    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF( IsRead__c ,IMAGE(&quot;/servlet.ImageServer?id=01530000000sjUK&amp;oid=00D30000000YukK&quot;,&quot;Open&quot;,10,10) ,
   IMAGE(&quot;/servlet.ImageServer?id=01530000000sjMt&amp;oid=00D30000000YukK&quot;,&quot;Closed&quot;,10,10))</formula>
